{
  "name": "kogitoapp-gitea",
  "version": "1.0.3",
  "author": "kogitoapp",
  "summary": "Manage Gitea with Puppet",
  "license": "Apache-2.0",
  "source": "https://github.com/kogitoapp/puppet-gitea.git",
  "project_page": "https://github.com/kogitoapp/puppet-gitea",
  "issues_url": "https://github.com/kogitoapp/puppet-gitea/issues",
  "tags": [
    "gitea",
    "git",
    "linux"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "7"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "7"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "16.04"
      ]
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 4.14.0 < 5.0.0"
    },
    {
      "name": "puppetlabs-inifile",
      "version_requirement": ">= 1.6.0 < 2.0.0"
    },
    {
      "name": "lwf-remote_file",
      "version_requirement": ">= 1.1.3 < 2.0.0"
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.0.0 < 5.0.0"
    }
  ]
}
