{
  "name": "kogitoapp-gitea",
  "version": "0.1.0",
  "author": "kogitoapp",
  "summary": "Manage Gitea with Puppet",
  "license": "Apache-2.0",
  "source": "https://github.com/kogitoapp/puppet-gitea.git",
  "project_page": "https://github.com/kogitoapp/puppet-gitea",
  "issues_url": "https://github.com/kogitoapp/puppet-gitea/issues",
  "tags": [
    "gitea",
    "git",
    "linux"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Fedora",
      "operatingsystemrelease": [
        "24"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "14.04",
        "16.04"
      ]
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 4.14.0 < 5.0.0"
    }
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": ">= 2015.2.0 <= 2016.5.0"
    },
    {
      "name": "puppet",
      "version_requirement": ">= 4.0.0 < 5.0.0"
    }
  ],
  "data_provider": "none"
}
